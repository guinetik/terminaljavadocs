<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.8.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.8.0 http://maven.apache.org/xsd/decoration-1.8.0.xsd"
         name="Terminal Javadocs Resources">

    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>2.0.0-M8</version>
    </skin>

    <custom>
        <fluidoSkin>
            <topBarEnabled>true</topBarEnabled>
            <sideBarEnabled>false</sideBarEnabled>
            <topBarIcon>
                <name>Terminal Javadocs</name>
                <alt>Terminal Javadocs</alt>
                <src>https://terminaljavadocs.guinetik.com/images/favicon.svg</src>
                <href>../index.html</href>
            </topBarIcon>
            <navBarStyle>navbar-dark</navBarStyle>
            <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
            <skipGenerationDate>true</skipGenerationDate>
        </fluidoSkin>
    </custom>

    <body>
        <head>
            <![CDATA[
                <link rel="icon" type="image/svg+xml" href="../images/favicon.svg" />
            ]]>
        </head>

        <breadcrumbs>
            <item name="Terminal Javadocs" href="../index.html"/>
            <item name="Resources" href="index.html"/>
        </breadcrumbs>

        <menu name="Overview">
            <item name="About" href="index.html"/>
        </menu>
        <menu ref="reports"/>

        <menu name="Parent">
            <item name="Back to Main Site" href="../index.html"/>
        </menu>
    </body>
</project>
