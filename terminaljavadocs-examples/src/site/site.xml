        </fluidoSkin>
    </custom>

    <body>
        <menu name="Overview">
            <item name="Introduction" href="index.html"/>
            <item name="Summary" href="summary.html"/>
        </menu>
        <menu name="Source">
            <item name="Source XRef" href="xref/index.html"/>
        </menu>
        <menu name="Documentation">
            <item name="Javadoc" href="apidocs/index.html"/>
        </menu>
        <menu ref="reports"/>
    </body>
</project>
